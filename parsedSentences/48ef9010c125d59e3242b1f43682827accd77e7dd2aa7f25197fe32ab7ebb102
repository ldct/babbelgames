  (SENT
    (VN (V Est) (CLS -ce) (VPP dingue))
    (VPinf (P d')
      (VN (VINF aider))
      (NP (DET les) (NC toxicos))
      (VPinf (P à)
        (VN (VINF décrocher))
        (PUNC ,)
        (VPinf (P à)
          (VN (VINF quitter))
          (NP (DET la) (NC rue)))))
    (PUNC ,)
    (VPinf
      (VN (CLO les) (VINF remettre))
      (MWADV (P sur) (DET les) (N rails)))
    (PUNC ?)))

